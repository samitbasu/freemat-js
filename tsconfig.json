{
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "alwaysStrict": true,
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true
    },
    "include": [
        "./src/**/*"
    ]
}
